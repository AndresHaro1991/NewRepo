<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppGps.MainPage">

    <StackLayout>
        <Label Text="Ubicacion"/>
        <Entry Text="Nombre Ubiacion" x:Name="txtNombreUbica"/>
        <Label Text="Latitud" Margin="0,24,0,0"/>
        <Entry Text="19.4343942" Keyboard="Numeric" x:Name="txtLatitud"/>
        <Label Text="Longitud" Margin="0,24,0,0" />
        <Entry Text="-99.1330824" x:Name="txtLongitud" Keyboard="Numeric" />
        <Button Text="Abrir Ubicaion" x:Name="btnAbrir" Clicked="btnAbrir_Clicked"/>
    </StackLayout>

</ContentPage>
